{
  "name": "elgibor-solution/laravel-botpress",
  "description": "A Laravel bridge for Botpress Community Edition API.",
  "keywords": [
    "laravel",
    "botpress",
    "bridge",
    "chat"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Elgibor Solution",
      "email": "info@elgibor-solution.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/http": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "ESolution\\Botpress\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ESolution\\Botpress\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ESolution\\Botpress\\BotpressServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
